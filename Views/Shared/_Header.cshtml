@using contactForm.VariableModels;
@{
    List<HeaderModels> test = new List<HeaderModels>() {
        new HeaderModels(title: "Home", actionName: "Index", controllerName: "Home"),
        new HeaderModels(title: "About", actionName: "Index", controllerName: "About")
    };
}

<header>
    <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
        <div class="container-fluid">
            <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Contact Form</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                    aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                <ul class="navbar-nav flex-grow-1">
                    @foreach (var items in test)
                    {
                        <li class="nav-item">
                            @Html.ActionLink( items.title,  items.actionName, items.controllerName, null, new {
                           @class = "nav-link text-dark"
                       })
                        </li>
                    }
                </ul>
            </div>
        </div>
    </nav>
</header>
